{
  "presets": [
    "react",
    "es2015",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-dynamic-import",
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": "less"
    }]
  ],
  "comments": false
}
